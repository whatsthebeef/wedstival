<%= form_for @user_session do |f| %>
   <div class="form-element">
      <%= h flash[:notice] %>
   </div>
   <div class="form-element">
      <%= f.text_field :username, placeholder: "Username" %>
   </div>
   <div class="form-element">
      <%= f.password_field :password, placeholder: "Password"  %>
   </div>
   <div class="form-element">
      <%= recaptcha_tags display:{theme:"white"}%>
   </div>
   <div class="form-element">
      <%= image_submit_tag "submit.png" %>
   </div>
<% end %>
