<script type="text/javascript">
   var fireEngine;
   initCanvas(function() {
      var sources = {
         fireEngineRight: "<%= asset_path 'fire_engine_right.png' %>",
         wheelsRight: "<%= asset_path 'wheels_right.png' %>",
         fireEngineLeft: "<%= asset_path 'fire_engine_left.png' %>",
         wheelsLeft: "<%= asset_path 'wheels_left.png' %>",
         fireStation: "<%= asset_path 'fire_station.png' %>",
         roofChicken: "<%= asset_path 'roof_chicken.png' %>",
         roofChickenDown: "<%= asset_path 'roof_chicken_left.png' %>",
         fireEngine: "<%= asset_path 'fire_engine.png' %>"
      };

      var fireEngine = new FireEngine(0, 0, 200, 200, 10); 
      var fireStation = [new FireStation((window.innerWidth/2-150), 
         0, 300, 300)];
      var fec = canvas(sources, "fire-engine-canvas", window.innerWidth, 
         200, [fireEngine], function(){
            if(fireEngine.absX() > window.innerWidth && !fireEngine.cleanUp) {
               fireEngine.cleanUp = true;
               fireStation.push(fireEngineParking);
            }
      });
      var fireEngineParking = new FireEngine(window.innerWidth, 135, 200, 200, -10);
      var fsc = canvas(sources, "fire-station-canvas", window.innerWidth, 300, 
         fireStation, function(){
            if(fireEngineParking.absX() < (window.innerWidth/2 - fireEngine.width/2 + 10)) {
               fireEngineParking.velocity = 0;
            }
      });

      var windUpCanvas = function(canvas) {
         if(canvas.height > 0) {
            window.setTimeout(function(){
               canvas.height--;
               windUpCanvas(canvas);
            }, 20);
         }
         else {
            fireStation.push(fireEngineParking);
         }
      }
   });


</script>
