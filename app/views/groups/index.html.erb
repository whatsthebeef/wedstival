<div id="description">
   <h3>Groups</h3>
   <% @groups.each do |group| %>
      <div class="group">
         <div>
            <h3><%= link_to group.name, group_path(group) %></h3>
         </div>
         <div>
            <%= render partial: "shared/group", object: group %>
         </div>
      </div>
   <% end %>      
   <div class="add-group">
   <% if params[:has_submitted].blank? %>
      <%= link_to image_tag("add_group.png"), new_group_path, class:"add" %>
   <% end %>      
      <%= link_to image_tag("cancel.png"), admin_path, class:"cancel" %>
   </div>

</div>
